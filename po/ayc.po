#  "Distancia"
#: activity/activity.info:2
msgid "Distance"
msgstr "Jaya"

#: activity.py:162
msgid "Atmosphere"
msgstr "Pacha phututi"

#: activity.py:165
msgid "Custom metric"
msgstr "Jaqi taña chani"

#: activity.py:187
msgid ""
"To measure the distance between two laptops, you must first share this "
"Activity."
msgstr ""
"Paya computadora jaya chanipa tupañatakixa, akïri wakichataru mayachasiñawa "
"wakisi."

#: activity.py:189
msgid "Press the button to measure the distance to another laptop"
msgstr "Yaqha computadora jaya chani utjatapa tupañataki akïri limt'ma"

#: activity.py:191
msgid "Preparing to measure distance"
msgstr "Jaya chanipa tupañataki wakichasiña"

#: activity.py:192
msgid "Ready to make a measurement.  Waiting for partner to be ready."
msgstr ""
"Jaya chanipa tupañataki wakichasita... Jaqi masisa wakichasiñapa suyt'äwi."

#: activity.py:194
msgid "Recording sound from each laptop."
msgstr "Sapa computadora sijiqipa katuqasa imaña"

#: activity.py:195
msgid "Processing recorded audio."
msgstr "Sapa computadora sijiqipa katuqasa imata istäwi"

#: activity.py:197
msgid "This activity already has two participants, so you cannot join."
msgstr "Akïri wakichatana paya jaqiwa irnaqi, ukhamata janiwa mantañjamati."

#: activity.py:200
msgid "Begin Measuring Distance"
msgstr "Jaya chani tupaña qalltaña"

#: activity.py:201
msgid "Stop Measuring Distance"
msgstr "Jaya chani tupaña tukuña"

#: activity.py:235 smoot_toolbar.py:99
#, python-format
msgid "Measured distance in %s"
msgstr "Jaya chani tupata chanipa %s"

#: activity.py:235 smoot_toolbar.py:30 smoot_toolbar.py:34 smoot_toolbar.py:77
#: smoot_toolbar.py:100
msgid "meters"
msgstr "chillqinaka"

#: atm_toolbars.py:65
msgid "Temperature (C): "
msgstr "Pacha phututi (°C): "

#: atm_toolbars.py:70
msgid "Relative Humidity (%): "
msgstr "Umïri mayjt'äwi (%): "

#: atm_toolbars.py:75
msgid "Speed of Sound (m/s): "
msgstr "Sijiqi ist'asïwi (m/s): "

#: calibration_toolbar.py:28
msgid "Calibration Offset (meters): "
msgstr "Askichäwi ch'amanchäwi (chillqinaka): "

#: smoot_toolbar.py:30 smoot_toolbar.py:35
msgid "centimeters"
msgstr "luk'ana chani"

#. TRANS: English units of measure
#: smoot_toolbar.py:32 smoot_toolbar.py:36
msgid "inches"
msgstr "paya luk'ana chani"

#: smoot_toolbar.py:32 smoot_toolbar.py:37
msgid "feet"
msgstr "kayunaka"

#: smoot_toolbar.py:32 smoot_toolbar.py:38
msgid "yards"
msgstr "waranaka"

#: smoot_toolbar.py:33 smoot_toolbar.py:39
msgid "custom units"
msgstr "sapa jaqi tupäwi"

#: smoot_toolbar.py:81
msgid "Choose a unit of measure:"
msgstr "Tapañataki maya tupäwi chhijllma"

#: smoot_toolbar.py:84
msgid "select units"
msgstr "Tupäwi ajllma"

#: smoot_toolbar.py:103
#, python-format
msgid "%(unit)20.2f %(name)s per meter"
msgstr "%(unit)20.2f %(name)s sapa qillqita"
