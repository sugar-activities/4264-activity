#: smoot_toolbar.py:100
msgid "meters"
msgstr "मीटर्स"

#: atm_toolbars.py:65
msgid "Temperature (C): "
msgstr "तापमान(C): "

#: atm_toolbars.py:70
msgid "Relative Humidity (%): "
msgstr "सबंधित नमी(%): "

#: atm_toolbars.py:75
msgid "Speed of Sound (m/s): "
msgstr "आवाज़ाची गती(मी/से): "

#: calibration_toolbar.py:28
msgid "Calibration Offset (meters): "
msgstr "अंशांत्मक समायोजन(मीटर्स) "

#: smoot_toolbar.py:30 smoot_toolbar.py:35
msgid "centimeters"
msgstr "सेंटिमीटर"

#. TRANS: English units of measure
#: smoot_toolbar.py:32 smoot_toolbar.py:36
msgid "inches"
msgstr "इंचेस"

#: smoot_toolbar.py:32 smoot_toolbar.py:37
msgid "feet"
msgstr "फुट"

#: smoot_toolbar.py:32 smoot_toolbar.py:38
msgid "yards"
msgstr "यार्ड"

#: smoot_toolbar.py:33 smoot_toolbar.py:39
msgid "custom units"
msgstr "स्वरचीत एकके"

#: smoot_toolbar.py:81
msgid "Choose a unit of measure:"
msgstr "मोजमाप करण्यासाठीचे एकक निवडा:"

#: smoot_toolbar.py:84
msgid "select units"
msgstr "एकके निवडा"

